[user]
        name = Peter Schwarz
	email = pschwarz@bitwise.io
[core]
       	autocrlf = input
	excludesfile = /Users/pschwarz/.gitignore
	editor = vim
[diff]
       	algorithm = patience
[push]
       	default = matching
[filter "media"]
		clean = git-media-clean %f
       	smudge = git-media-smudge %f
[filter "lfs"]
	clean = git-lfs clean %f
	smudge = git-lfs smudge %f
	required = true
[alias]
[alias]
	slog = log --pretty=format:\"%h - %an, %ar : %s\" --simplify-merges --no-merges
